{
    "name": "sofyco/doctrine-mongodb-document-denormalizer-bundle",
    "type": "symfony-bundle",
    "description": "Denormalize document object in serializer component",
    "keywords": ["php", "symfony", "bundle", "doctrine", "mongodb", "document", "denormilizer", "serializer"],
    "homepage": "https://github.com/sofyco/doctrine-mongodb-document-denormalizer-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sofy Korzhova",
            "email": "sofykorzhova@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-mongodb": "*",
        "doctrine/mongodb-odm-bundle": "^4.5",
        "symfony/framework-bundle": "^6.2",
        "symfony/property-access": "^6.2",
        "symfony/serializer": "^6.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.6",
        "symfony/yaml": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Sofyco\\Bundle\\Doctrine\\MongoDB\\DocumentDenormalizerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sofyco\\Bundle\\Doctrine\\MongoDB\\DocumentDenormalizerBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
